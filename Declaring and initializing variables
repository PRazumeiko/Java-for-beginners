package com.company;

public class Main {

    public static void main(String[] args) {
      int number = 123;
      double x = 36.6;
     char sym = 'A';
     boolean state = true;
        System.out.println("Variables used:");
        System.out.println("Integer:" + number);
        System.out.println("Real variable:" + x);
        System.out.println("Character variable:" + sym);
        System.out.println("Boolean variable:" + state);
    }
}
